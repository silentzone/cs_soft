/* layout */

body,html{ font-family: "Microsoft YaHei","微软雅黑"; margin: 0; padding: 0;} 
ul li { list-style: none}
.desktop { background:url("../img/desktop_bg.jpg") repeat-x 0 0; }
.section .top { font-size: 15px; font-weight: bold; color: #222; border-bottom:#ececec solid 1px; height:32px; line-height:32px; width: 100%; background:none; }
.top { font-size: 15px; font-weight: bold; color: #fff; border-bottom:#ececec solid 1px; height:32px; line-height:32px; width: 100%; background:url("../img/dialog/title.jpg") repeat-x; }

.top .title { padding-left: 10px; }

.box-content { padding:15px 0;}
.bs-docs-sidenav {
    background-color: #FFFFFF;
    border-radius: 6px 6px 6px 6px;
    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.067);
    margin: 15px 0 0;
    padding: 0; 
}  
.bs-docs-sidenav > li > a {
    border: 1px solid #E5E5E5;
    display: block;
    margin: 0 0 -1px;
    padding: 8px 14px;
}
.bs-docs-sidenav > li:first-child > a {
    border-radius: 6px 6px 0 0;
}
.bs-docs-sidenav > li:last-child > a {
    border-radius: 0 0 6px 6px;
}
.bs-docs-sidenav > .active > a {
    border: 0 none;
    box-shadow: 1px 0 0 rgba(0, 0, 0, 0.1) inset, -1px 0 0 rgba(0, 0, 0, 0.1) inset;
    padding: 9px 15px;
    position: relative;
    text-shadow: 0 1px 0 rgba(0, 0, 0, 0.15);
    z-index: 2;
}
.bs-docs-sidenav .icon-chevron-right {
    float: right;
    margin-right: -6px;
    margin-top: 2px;
    opacity: 0.25;
}
.bs-docs-sidenav > li > a:hover {
    background-color: #F5F5F5;
}
.bs-docs-sidenav a:hover .icon-chevron-right {
    opacity: 0.5;
}
.bs-docs-sidenav .active .icon-chevron-right, .bs-docs-sidenav .active a:hover .icon-chevron-right {
    background-image: url("../img/glyphicons-halflings-white.png");
    opacity: 1;
}

